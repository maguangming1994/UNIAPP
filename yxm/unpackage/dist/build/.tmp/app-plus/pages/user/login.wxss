.passport_page {
  color: #fff;
}
.passport_page .mhcms-top-nav-bar {
    font-size: 28 upx;
    padding: 0 60 upx;
}
.passport_page .page-body {
    padding: 150 upx 60 upx 30 upx;
}
.passport_page .page-body .page-title-txt {
      margin-bottom: 45 upx;
}
.passport_page .page-body .page-action .uni-form-item {
      padding: 30 upx 0;
      border-bottom: solid 2px #fff;
      text-align: center;
}
.passport_page .page-body .btn {
      color: #ff7853;
      height: 100 upx;
      line-height: 100 upx;
      border-radius: 4 upx;
      margin-top: 30 upx;
}
.passport_page .page-body .form-label {
      font-size: 30 upx;
}
.passport_page .mhcms-bottom-nav-bar {
    position: absolute;
    bottom: 0;
    font-size: 26 upx;
    width: 100%;
    text-align: center;
    line-height: 80 upx;
    font-weight: 300;
    left: 0;
}
.input-placeholder {
  color: #fafafa;
}

