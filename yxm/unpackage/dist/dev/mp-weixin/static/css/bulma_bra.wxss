/* bta-tabs */
.bra-active:after{content:'';left:25%;pointer-events:none;position:absolute;bottom:0;width:50%;}
.bra-active:before{content:'';left:0;pointer-events:none;position:absolute;top:0;width:90%;}
.bta-tabs .bta-tab{position:relative}
.bta-tabs .bra-active:after{border-bottom:2px solid;}
.bta-tabs.orange .bra-active{color:#ff5e00;}
.bta-tabs.orange .bra-active:after{border-color:#ff5e00;}
/** bra-btn**/
.bra-btn.normal{font-size:14px;height:38px;line-height:38px;}
.bra-btn.mini{font-size:12px;height:24px;line-height:24px;padding: 0 5px;}
.bra-btn{border:none;cursor:pointer;text-align:center;white-space:nowrap;position:relative;overflow:hidden;}
.bra-btn.orange{background:#ff5e00;color:#fff}
.bra-btn.bulla{background:#00d1b2;color:#fff}
.bra-btn.grey{background: #e6e6e6;color: #9c9c9c;}
.bra-btn.round{display: block;
    border: solid 1px #ccc;
    text-align: center;
    margin: auto;
}

.bra-btn.round.normal{height: 50px;width: 50px;border-radius: 50%;padding:0;}
.bra-btn.round.normal .iconfont{
    font-size:28px;
    line-height:50px;
}
